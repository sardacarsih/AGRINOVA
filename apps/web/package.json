{
  "name": "@agrinova/web",
  "version": "1.0.0",
  "description": "Agrinova Web Dashboard - Next.js dashboard for palm oil harvest management",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo -p 3000",
    "build": "cross-env BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true next build --turbo",
    "build:prod": "cross-env BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true NODE_ENV=production next build --turbo",
    "build:standalone": "cross-env BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true NODE_ENV=production NEXT_OUTPUT_STANDALONE=true next build --turbo",
    "start": "next start",
    "start:prod": "cross-env NODE_ENV=production next start -p 3000",
    "lint": "cross-env BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true eslint .",
    "lint:fix": "cross-env BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true eslint . --fix",
    "type-check": "tsc --noEmit",
    "check:company-admin-ui": "node scripts/check-company-admin-ui.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "package:standalone": "powershell -ExecutionPolicy Bypass -File ./scripts/package-standalone.ps1",
    "release:server": "powershell -ExecutionPolicy Bypass -File ./scripts/release-production-runtime.ps1",
    "release:server:skip-build": "powershell -ExecutionPolicy Bypass -File ./scripts/release-production-runtime.ps1 -SkipBuild",
    "release:standalone": "npm run build:standalone && npm run package:standalone",
    "deploy:prod": "npm run build:prod && npm run start:prod"
  },
